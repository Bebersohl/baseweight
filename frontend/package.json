{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.9.4",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.44",
    "@reach/router": "^1.3.3",
    "@reduxjs/toolkit": "^1.2.5",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^9.4.1",
    "@testing-library/user-event": "^10.0.0",
    "@types/color": "^3.0.1",
    "@types/jest": "^25.1.3",
    "@types/node": "^13.7.7",
    "@types/papaparse": "^5.0.3",
    "@types/reach__router": "^1.3.0",
    "@types/react": "^16.9.23",
    "@types/react-beautiful-dnd": "^12.1.1",
    "@types/react-dom": "^16.9.5",
    "@types/react-redux": "^7.1.7",
    "@types/shortid": "0.0.29",
    "@use-it/interval": "^0.1.3",
    "@welldone-software/why-did-you-render": "^4.0.5",
    "autosuggest-highlight": "^3.1.1",
    "clone-deep": "^4.0.1",
    "color": "^3.1.2",
    "filenamify": "^4.1.0",
    "firebase": "^7.9.2",
    "insane": "^2.6.2",
    "is-url": "^1.2.4",
    "marked": "^4.0.10",
    "papaparse": "^5.1.1",
    "react": "^16.13.0",
    "react-beautiful-dnd": "^13.0.0",
    "react-beforeunload": "^2.1.0",
    "react-dom": "^16.13.0",
    "react-google-button": "^0.7.0",
    "react-redux": "^7.2.0",
    "react-scripts": "3.4.0",
    "redux-logger": "^3.0.6",
    "shortid": "^2.2.15",
    "source-map-explorer": "^2.3.1",
    "typescript": "^3.8.2"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "deploy": "npm run build && firebase deploy",
    "deploy:prod": "npm run build && firebase deploy -P production",
    "shutdown": "firebase hosting:disable",
    "shutdown:prod": "firebase hosting:disable -P production",
    "deploy:rules": "firebase deploy --only firestore:rules",
    "deploy:rules:prod": "firebase deploy --only firestore:rules -P production"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "not IE 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8",
    "prettier": "^1.19.1"
  }
}
